{
  "name": "next-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "update:tcm": "tcm -p 'src/**/*.module.css' . && yarn lint:fix",
    "lint-ts": "eslint 'src/**/*.{ts,tsx}'",
    "lint-style": "stylelint 'src/**/*.css'",
    "lint-ts:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint-style:fix": "stylelint 'src/**/*.css' --fix",
    "lint:check": "yarn lint-ts & yarn lint-style",
    "lint:fix": "yarn lint-ts:fix && yarn lint-style:fix && yarn format",
    "format": "yarn prettier --write 'src/**/*.{ts,tsx,css}'",
    "lint": "next lint"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dayjs": "^1.11.10",
    "github-markdown-css": "^5.4.0",
    "highlight.js": "^11.9.0",
    "marked": "^11.0.0",
    "marked-highlight": "^2.0.8",
    "next": "13.4.19",
    "normalize.css": "^8.0.1",
    "react": "^18",
    "react-dom": "^18",
    "react-scroll": "^1.9.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@storybook/cli": "^7.6.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-scroll": "^1.8.10",
    "@types/react-transition-group": "^4.4.9",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "stylelint": "^16.2.1",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard": "^36.0.0",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5",
    "typescript-eslint": "^7.3.1"
  }
}
